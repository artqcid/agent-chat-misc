{
  "version": "2.0.0",
  "tasks": [
    {
      "type": "npm",
      "script": "watch",
      "group": "build",
      "presentation": {
        "panel": "shared",
        "reveal": "never"
      },
      "problemMatcher": "$tsc-watch",
      "isBackground": true,
      "runOptions": {
        "runOn": "folderOpen"
      }
    },
    {
      "type": "npm",
      "script": "pretest",
      "group": "build",
      "presentation": {
        "panel": "shared",
        "reveal": "never"
      },
      "problemMatcher": "$tsc"
    },
    {
      "label": "AGENT: Run ALL Tests",
      "type": "npm",
      "script": "test",
      "group": {
        "kind": "test",
        "isDefault": true
      },
      "presentation": {
        "echo": true,
        "reveal": "always",
        "focus": false,
        "panel": "shared"
      },
      "problemMatcher": []
    }
  ]
}
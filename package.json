{
  "name": "agent-chat-misc",
  "displayName": "Agent Chat Misc",
  "description": "A VS Code extension providing a chat agent with GUI similar to GitHub Copilot, supporting local and remote LLMs, MCP, RAG, and Embedding.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:agentChat.openChat"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "agentChat.openChat",
        "title": "Open Agent Chat"
      },
      {
        "command": "agentChat.openSettings",
        "title": "Open Agent Chat Settings"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "agentChatView",
          "name": "Agent Chat",
          "icon": "$(comment-discussion)"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/index.js",
    "test:frontend": "jest",
    "test:all": "npm run test && npm run test:frontend"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.4"
  }
}
